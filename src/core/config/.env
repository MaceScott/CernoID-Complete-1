ENV_VARIABLES=example 

# System
CERNOID_ENV=development
CERNOID_DEBUG=true

# Database
CERNOID_DB_USER=cernoid_user
CERNOID_DB_PASSWORD=your_secure_password
CERNOID_DB_HOST=localhost
CERNOID_DB_PORT=5432
CERNOID_DB_NAME=cernoid_db

# Security
CERNOID_JWT_SECRET=your_jwt_secret_key
CERNOID_SENTRY_DSN=your_sentry_dsn

# Redis
CERNOID_REDIS_HOST=localhost
CERNOID_REDIS_PORT=6379
CERNOID_REDIS_PASSWORD=your_redis_password

# Recognition
CERNOID_RECOGNITION_GPU_ENABLED=true
CERNOID_RECOGNITION_CONFIDENCE_THRESHOLD=0.85 
